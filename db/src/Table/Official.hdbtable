COLUMN TABLE "Official" (
	"PNR"		NVARCHAR(3) UNIQUE NOT NULL COMMENT 'Pers number',
	"NAME"		NVARCHAR(50)		COMMENT 'Name',
	"OVERTIME"	INT			NULL	COMMENT 'OverTime',
	"SALARY"	NVARCHAR(3)			COMMENT 'Salary',
	"MANAGER"	NVARCHAR(3)	NULL	COMMENT 'Manager',
	PRIMARY KEY ("PNR")
)
COMMENT 'Official table'